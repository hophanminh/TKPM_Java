<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane prefHeight="440.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <VBox>
         <children>
            <VBox prefHeight="48.0" prefWidth="600.0">
               <children>
                  <HBox alignment="CENTER_LEFT">
                     <children>
                        <HBox alignment="CENTER_LEFT" prefHeight="48.0" prefWidth="219.0">
                           <children>
                              <ComboBox fx:id="ssComboBox" prefWidth="200.0" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <ComboBox fx:id="ieComboBox" prefWidth="200.0" />
                           </children>
                        </HBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS">
               <children>
                  <VBox prefHeight="44.0" prefWidth="600.0">
                     <children>
                        <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0">
                           <children>
                              <HBox prefHeight="44.0" prefWidth="422.0" spacing="10.0">
                                 <children>
                                    <HBox alignment="CENTER_LEFT" prefHeight="43.0" prefWidth="364.0">
                                       <children>
                                          <TextField fx:id="searchText" prefHeight="26.0" prefWidth="361.0" />
                                       </children>
                                    </HBox>
                                    <HBox alignment="CENTER_LEFT" prefHeight="0.0" prefWidth="100.0">
                                       <children>
                                           <Button fx:id="searchButton" mnemonicParsing="false" text="Search">
                                             <font>
                                                <Font size="15.0" />
                                             </font></Button>
                                       </children>
                                    </HBox>
                                 </children>
                              </HBox>
                              <Region maxWidth="1.7976931348623157E308" prefHeight="0.0" prefWidth="0.0" HBox.hgrow="ALWAYS" />
                              <HBox alignment="CENTER" prefHeight="0.0" prefWidth="100.0">
                                 <children>
                                    <Button fx:id="sendEmailButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Send Email">
                                       <font>
                                          <Font size="15.0" />
                                       </font></Button>
                                 </children>
                              </HBox>
                              <HBox alignment="CENTER" prefHeight="0.0" prefWidth="100.0">
                                 <children>
                                    <Button fx:id="printButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Print">
                                       <font>
                                          <Font size="15.0" />
                                       </font></Button>
                                 </children>
                              </HBox>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <VBox prefHeight="269.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
                     <children>
                        <TableView fx:id="tableView" maxHeight="1.7976931348623157E308" prefHeight="292.0" prefWidth="600.0">
                           <VBox.margin>
                              <Insets bottom="5.0" top="5.0" />
                           </VBox.margin>
                        </TableView>
                     </children>
                  </VBox>
               </children>
            </VBox>
            <VBox prefHeight="27.0" prefWidth="600.0">
               <children>
                  <HBox prefHeight="0.0" prefWidth="600.0" spacing="10.0">
                     <children>
                        <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="100.0">
                           <children>
                              <Button fx:id="backToMainButton" mnemonicParsing="false" prefHeight="31.0" prefWidth="79.0" text="Close">
                                 <font>
                                    <Font size="15.0" />
                                 </font></Button>
                           </children>
                        </HBox>
                        <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                        <HBox alignment="CENTER_RIGHT" prefHeight="30.0" prefWidth="100.0">
                           <children>
                              <Button fx:id="addButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Add">
                                 <font>
                                    <Font size="15.0" />
                                 </font></Button>
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="30.0" prefWidth="100.0">
                           <children>
                              <Button fx:id="deleteButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Delete">
                                 <font>
                                    <Font size="15.0" />
                                 </font></Button>
                           </children>
                        </HBox>
                     </children>
                  </HBox>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </VBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</GridPane>
