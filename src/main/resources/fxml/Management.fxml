<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <VBox>
         <children>
            <VBox prefHeight="48.0" prefWidth="600.0">
               <children>
                  <HBox alignment="CENTER_LEFT">
                     <children>
                        <HBox alignment="CENTER" prefHeight="48.0" prefWidth="219.0">
                           <children>
                              <ComboBox fx:id="ssComboBox" prefWidth="150.0" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <ComboBox fx:id="ieComboBox" prefWidth="150.0" />
                           </children>
                        </HBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox>
               <children>
                  <VBox prefHeight="44.0" prefWidth="600.0">
                     <children>
                        <HBox prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <HBox prefHeight="44.0" prefWidth="525.0">
                                 <children>
                                    <HBox alignment="CENTER_LEFT" prefHeight="43.0" prefWidth="364.0">
                                       <children>
                                          <TextField fx:id="searchText" prefHeight="26.0" prefWidth="361.0" />
                                       </children>
                                    </HBox>
                                    <HBox alignment="CENTER_LEFT" prefHeight="43.0" prefWidth="59.0">
                                       <children>
                                           <Button fx:id="searchButton" mnemonicParsing="false" text="Search" />
                                       </children>
                                    </HBox>
                                 </children>
                              </HBox>
                              <HBox alignment="CENTER">
                                 <children>
                                    <Button fx:id="printButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="67.0" text="Print" />
                                 </children>
                              </HBox>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <VBox prefHeight="269.0" prefWidth="600.0">
                     <children>
                        <TableView fx:id="tableView" prefHeight="292.0" prefWidth="600.0">
                           <VBox.margin>
                              <Insets bottom="5.0" top="5.0" />
                           </VBox.margin>
                        </TableView>
                     </children>
                  </VBox>
               </children>
            </VBox>
            <VBox prefHeight="27.0" prefWidth="600.0">
               <children>
                  <HBox prefHeight="0.0" prefWidth="600.0">
                     <children>
                        <HBox alignment="CENTER_LEFT" prefHeight="26.0" prefWidth="295.0">
                           <children>
                              <Button fx:id="backToMainButton" mnemonicParsing="false" text="Back to main" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT" prefHeight="26.0" prefWidth="157.0">
                           <children>
                              <Button fx:id="addButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="111.0" text="Add" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="39.0" prefWidth="82.0">
                           <children>
                              <Button fx:id="deleteButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="61.0" text="Delete" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="26.0" prefWidth="90.0">
                           <children>
                              <Button fx:id="updateButton" mnemonicParsing="false" text="Update data" />
                           </children>
                        </HBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</GridPane>
