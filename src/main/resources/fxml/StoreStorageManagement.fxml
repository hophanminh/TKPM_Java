<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane prefHeight="440.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <VBox>
         <children>
            <VBox prefHeight="48.0" prefWidth="600.0">
               <children>
                  <HBox alignment="CENTER_LEFT">
                     <children>
                        <HBox alignment="CENTER_LEFT" prefHeight="48.0" prefWidth="219.0">
                           <children>
                              <ComboBox fx:id="typeField" prefWidth="200.0" />
                           </children>
                        </HBox>
                        <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                        <Button fx:id="defaultButton" mnemonicParsing="false" text="Chọn cửa hàng mặc định">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS">
               <children>
                  <BorderPane fx:id="borderPane" maxHeight="1.7976931348623157E308" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
            <VBox prefHeight="27.0" prefWidth="600.0">
               <children>
                  <HBox prefHeight="0.0" prefWidth="600.0" spacing="10.0">
                     <children>
                        <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="100.0">
                           <children>
                              <Button fx:id="backToMainButton" mnemonicParsing="false" prefHeight="31.0" prefWidth="79.0" text="Trở về">
                                 <font>
                                    <Font size="15.0" />
                                 </font></Button>
                           </children>
                        </HBox>
                        <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                        <HBox alignment="CENTER_RIGHT" prefHeight="30.0" prefWidth="100.0">
                           <children>
                              <Button fx:id="addButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Xác nhận">
                                 <font>
                                    <Font size="15.0" />
                                 </font></Button>
                           </children>
                        </HBox>
                     </children>
                  </HBox>
               </children>
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </VBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</GridPane>
